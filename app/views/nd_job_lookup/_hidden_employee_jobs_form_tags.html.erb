<% employee_jobs.each_with_index do |job, index| %>
  <%= hidden_field_tag "job_#{index}_title", job['title'] %>
  <%= hidden_field_tag "job_#{index}_orgn_code", job['orgn_code'] %>
  <%= hidden_field_tag "job_#{index}_orgn_title", job['orgn_title'] %>
  <%= hidden_field_tag "job_#{index}_primary", job['primary'] %>
  <%= hidden_field_tag "job_#{index}_status", job['status'] %>
  <%= hidden_field_tag "job_#{index}_posn", job['posn'] %>
  <%= hidden_field_tag "job_#{index}_suffix", job['suffix'] %>
  <%= hidden_field_tag "job_#{index}_start", job['start'] %>
  <%= hidden_field_tag "job_#{index}_end", job['end'] %>
<% end %>
